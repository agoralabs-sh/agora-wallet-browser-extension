{
  "manifest_version": 3,
  "permissions": ["activeTab", "alarms", "storage", "unlimitedStorage"],
  "action": {
    "default_title": "Kibisis"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "matches": ["*://*/*"],
      "resources": ["algorand-provider.js"]
    }
  ]
}
